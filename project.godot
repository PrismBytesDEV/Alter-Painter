; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="AlterPainter"
config/version="0.0.3"
run/main_scene="res://Scenes/AppInterface.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0.11, 0.11, 0.11, 1)
boot_splash/image="res://AlterPainterSplashScreen.png"
boot_splash/fullsize=false
config/icon="res://icon.png"
config/windows_native_icon="res://Alter-painter-logo.ico"

[debug]

file_logging/enable_file_logging=true
file_logging/log_path="user://logs/AlterPainter/app.log"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2

[editor_plugins]

enabled=PackedStringArray("res://addons/ReorderableContainer/plugin.cfg", "res://addons/dockable_container/plugin.cfg", "res://addons/zylann.editor_debugger/plugin.cfg")

[filesystem]

import/blender/enabled=false
